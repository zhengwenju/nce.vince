<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.zhy.android.percent.support.PercentRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/node_item_vf"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_10"
        app:layout_heightPercent="24%w">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_textSizePercent="4.5%w"
            app:layout_marginLeftPercent="5.79%w"
            app:layout_marginTopPercent="1.2%w"
            android:text="@string/node24"
            android:textColor="@color/white"
            />
        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_textSizePercent="3.5%w"
            app:layout_marginLeftPercent="5.79%w"
            android:textColor="@color/white"
            app:layout_marginTopPercent="16.2%w"
            android:text="@string/node27"
            />
        <TextView
            android:id="@+id/type_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_textSizePercent="3.5%w"
            android:text="@string/node26"
            android:textColor="@color/white"
            app:layout_marginLeftPercent="5.79%w"
            app:layout_marginTopPercent="9.5%w"
            />
        <TextView
            android:id="@+id/number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_textSizePercent="3.5%w"
            android:text="0.00"
            app:layout_marginLeftPercent="22.79%w"
            app:layout_marginTopPercent="9.5%w"
            android:textColor="@color/white6"
            />
        <TextView
            android:id="@+id/surplus_quantity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_textSizePercent="3.5%w"
            android:text="1.00"
            app:layout_marginLeftPercent="31.89%w"
            app:layout_marginTopPercent="16.2%w"
            android:textColor="@color/white6"
            />
        <TextView
            android:text="@string/node25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_textSizePercent="3.5%w"
            android:textColor="@color/white"
            app:layout_marginRightPercent="5.79%w"
            android:layout_alignParentRight="true"
            app:layout_marginTopPercent="9%w"
            />
        <TextView
            android:id="@+id/time"
            android:text="2000/01/01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_textSizePercent="3.5%w"
            app:layout_marginRightPercent="5.79%w"
            android:layout_alignParentRight="true"
            app:layout_marginTopPercent="15%w"
            android:textColor="@color/white6"
            android:textStyle="bold"
            />
        <!--<ImageView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/white1"
            android:layout_alignParentBottom="true"
            app:layout_marginLeftPercent="4%w"
            app:layout_marginRightPercent="4%w"/>-->
    </com.zhy.android.percent.support.PercentRelativeLayout>
</com.zhy.android.percent.support.PercentRelativeLayout>